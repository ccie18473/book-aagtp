void setup ()
{
  size (400, 300);
  background (255);
}

void draw ()
{
  fill (255, 0, 0); ellipse (100, 50, 50, 50);
  fill (0, 255, 0); ellipse (200, 50, 50, 50);
  fill (0, 0, 255); ellipse (300, 50, 50, 50);
  fill (255, 255, 0); ellipse (100, 150, 50, 50);
  fill (255, 0, 255); ellipse (200, 150, 50, 50);
  fill (0, 255, 255); ellipse (300, 150, 50, 50);
  fill (64, 64, 64); ellipse (100, 250, 50, 50);
  fill (128, 128, 128); ellipse (200, 250, 50, 50);
  fill (196, 196, 196); ellipse (300, 250, 50, 50);
}
